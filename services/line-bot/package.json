{
	"name": "line-bot",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/app.js",
		"dev": "prisma generate && tsx watch src/app.ts",
		"test": "jest",
		"setup-rich-menu": "tsx scripts/setup-rich-menu.ts",
		"lint": "biome lint ./src",
