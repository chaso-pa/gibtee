		"@types/mime-types": "^2.1.4",
		"@types/node": "^22.15.15",
		"@types/uuid": "^10.0.0",
		"nodemon": "^3.1.10",
		"prisma": "^6.7.0",
		"ts-node": "^10.9.2",
