		"@line/bot-sdk": "^9.9.0",
		"@prisma/client": "^6.7.0",
		"@stripe/stripe-js": "^7.3.0",
		"axios": "^1.9.0",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"ejs": "^3.1.9",
		"express": "^5.1.0",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"mime-types": "^3.0.1",
		"stripe": "^18.1.0",
		"uuid": "^11.1.0",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/cors": "^2.8.17",
